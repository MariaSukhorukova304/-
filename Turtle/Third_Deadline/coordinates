import turtle as t


with open('coord.txt') as c:
    
    for i in list(input('type number')):
        i = int(i)
        a = c.readlines()
    for j in range(len(a[i])):
        if a[i][j] > 20:
            t.left(a[i][j])
        elif a[i][j] == 0:
            t.up()
        elif a[i][j] == 1:
            t.down()
        else:
            t.forward(a[i][j])